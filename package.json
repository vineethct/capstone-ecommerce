    "plop": "^4.0.1",
    "postcss": "^8",
    "prettier": "^3.3.0",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "storybook": "^8.1.6",
    "tailwindcss": "^3.4.4",
    "typescript": "^5",
    "webpack": "^5.91.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,yml,md,css}": [
      "prettier --write"
    ],
    "*.{ts,tsx,js,jsx}": [
      "eslint --max-warnings 0"
    ]
  }
