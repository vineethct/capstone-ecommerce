{
  "name": "candt-nextjs-template",
  "packageManager": "yarn@4.1.1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "new:component": "yarn plop --plopfile ./.plop/plopfile.mjs",
    "dependency-graph": "npx skott --displayMode=webapp --trackThirdPartyDependencies --trackBuiltinDependencies --cwd=./src",
    "prepare": "husky"
  },
  "dependencies": {
    "@radix-ui/themes": "^3.0.2",
    "@t3-oss/env-nextjs": "^0.10.1",
    "jiti": "^1.21.0",
    "next": "14.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.23.5"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^8",
    "@storybook/addon-designs": "^8",
    "@storybook/addon-essentials": "^8",
    "@storybook/addon-interactions": "^8",
    "@storybook/addon-links": "^8",
    "@storybook/addon-onboarding": "^8",
    "@storybook/blocks": "^8",
    "@storybook/nextjs": "^8",
    "@storybook/react": "^8",
    "@storybook/test": "^8",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-sonarjs": "^0.25.1",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "eslint-plugin-unicorn": "^52.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "plop": "^4.0.1",
    "postcss": "^8",
    "prettier": "3.2.5",
    "storybook": "^8",
    "tailwindcss": "^3.4.3",
    "typescript": "^5",
    "webpack": "^5.91.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md}": [
      "prettier --write",
      "eslint --max-warnings 0 ."
    ]
  }
}
