    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "firebase": "^11.2.0",
    "graphql": "^16.10.0",
    "graphql-codegen": "^0.4.0",
    "graphql-request": "^7.1.2",
    "graphql-tag": "^2.12.6",
